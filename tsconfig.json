{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "strictNullChecks": true,
    "strict": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "importsNotUsedAsValues": "remove"
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
